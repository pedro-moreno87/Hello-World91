<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Maddison Suite">

	<listeners>
		<listener class-name="com.maddison.utils.listeners.TestListener"></listener>
		<listener class-name="com.maddison.utils.listeners.AnnotationTransformer"></listener>
	</listeners>

	<test name="Test Account">
		<classes>
			<class name="com.maddison.tests.AccountTest" />
		</classes>
	</test> <!--
	Test -->

	<test name="Test Login">
		<classes>
			<class name="com.maddison.tests.LoginTest" />
		</classes>
	</test>
	
	<test name="Register Login">
		<classes>
			<class name="com.maddison.tests.RegisterTest" />
		</classes>
	</test>

</suite> <!--
Suite -->
